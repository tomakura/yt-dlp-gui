{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default permissions for the app",
  "windows": [
    "*"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "dialog:default",
    "http:default",
    "process:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "yt-dlp",
          "cmd": "yt-dlp",
          "sidecar": false
        },
        {
          "name": "ffmpeg",
          "cmd": "ffmpeg",
          "sidecar": false
        },
        {
          "name": "ffprobe",
          "cmd": "ffprobe",
          "sidecar": false
        }
      ]
    },
    "shell:allow-open"
  ]
}